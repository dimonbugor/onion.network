<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <gradient
                android:type="linear"
                android:angle="0"
                android:startColor="#59EEF7FF"
                android:centerColor="#33FFFFFF"
                android:endColor="#146F7BFF" />
            <corners android:radius="24dp" />
            <stroke
                android:width="1dp"
                android:color="#335BC4FF" />
        </shape>
    </item>
    <item android:top="4dp" android:bottom="4dp" android:left="6dp" android:right="6dp">
        <shape android:shape="rectangle">
            <solid android:color="#0DFFFFFF" />
            <corners android:radius="18dp" />
        </shape>
    </item>
</layer-list>
